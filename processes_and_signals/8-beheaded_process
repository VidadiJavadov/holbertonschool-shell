#!/usr/bin/env bash
# Script to silently kill the 7-highlander process and output "OK"

# Find the PID of the 7-highlander process
pid=$(pgrep -f "7-highlander")

# If process exists, kill it silently
if [[ -n "$pid" ]]; then
    kill -9 "$pid" &>/dev/null
fi

# Output exactly "OK" with no extra whitespace
printf "OK\n"
